{
  "schemaVersion": 1,
  "id": "schnabelheads",
  "version": "${version}",

  "name": "Schnabel Heads",
  "description": "Gorgeous Schnabel-Miniblocks to complete your builds",
  "authors": [
    "Schnabelborg"
  ],
  "contact": {
    "homepage": "https://github.com/Schnabelborg/fabric-schnabel-heads",
    "sources": "https://github.com/Schnabelborg/fabric-schnabel-heads"
  },

  "license": "CC0-1.0",
  "icon": "assets/schnabelheads/icon.png",

  "environment": "client",
  "entrypoints": {
