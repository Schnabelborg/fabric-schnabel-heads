  "suggests": {
    "another-mod": "*"
  },
  "mixins": [
    "schnabelheads.mixins.json"
  ],
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_918": ["org/schnabelb/heads/injection/ItemRendererScaled"]
